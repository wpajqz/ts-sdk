{
  "name": "ts-linker-sdk",
  "version": "1.0.6",
  "description": "linker's ts sdk",
  "main": "dist/index.js",
  "types": "dist/types/index.d.ts",
  "scripts": {
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
    "start": "webpack-dev-server --config scripts/webpack/webpack.config.dev.js",
    "test": "jest --config=scripts/jest/jest.config.js",
    "build": "npm run clean && npm run build:cjs && npm run build:umd",
    "build:cjs": "tsc --project ./tsconfig.cjs.json",
    "build:umd": "NODE_ENV=production webpack --config scripts/webpack/webpack.config.umd.js",
    "clean": "rimraf dist"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/wpajqz/ts-sdk.git"
  },
  "author": "Paul",
  "license": "private",
  "dependencies": {
    "crypto-js": "^3.1.9-1",
    "node-int64": "^0.4.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-proposal-object-rest-spread": "^7.4.4",
    "@babel/plugin-transform-runtime": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-typescript": "^7.3.3",
    "@babel/runtime": "^7.4.4",
    "@types/crypto-js": "^3.1.43",
    "@types/jest": "^24.0.11",
    "@types/node-int64": "^0.4.29",
    "@typescript-eslint/eslint-plugin": "^1.7.0",
    "@typescript-eslint/parser": "^1.7.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^8.0.5",
    "babel-preset-env": "^1.7.0",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^2.1.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.5.0",
    "webpack-dev-server": "^3.3.1",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "typescript": "3.5",
    "webpack": "^4.30.0",
    "webpack-cli": "^3.3.2"
  },
  "files": [
    "dist/"
  ]
}
